path: <%= @god_path %>
xmpp_config:
  domain: <%= @god_xmpp_domain %>
  resource: <%= @god_xmpp_resource %>
  bot_list:
<% @god_xmpp_config.each do |xmpp| %>
    - {username: <%= xmpp %>}
<% end %>

mail_domain: <%= @god_mail_domain %>
mail_user: <%= @god_mail_user %>
mail_pw: <%= @god_mail_pw %>
notify_list:
<% @god_notify_list.each do |notify| %>
  - {name: <%= notify['name'] %>, email: <%= notify['email'] %>}
<% end %>
